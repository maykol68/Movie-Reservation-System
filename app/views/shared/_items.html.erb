<div class="w-full text-white">
    <% if current_user %>
    <div class=" ">
      <%= link_to t('Sign_Out'), destroy_user_session_path, data: { turbo_method: :delete, turbo_confirm: t('common.confirm') }  %>
    </div>
    <% end %>
    <div class="w-full  underline">
      <% unless current_user %>
        <%= link_to t('.Sign_In'), new_user_session_path, class: "text-lg ml-10" %>
        <%= link_to t('.Sign_Up'), new_user_registration_path, class: "text-lg ml-10" %>
      <% end %>
    </div>
  </div>
  